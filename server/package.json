{
  "name": "motstanden-web",
  "version": "1.0.0",
  "description": "Motstanden web page",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serverdeploy": "npm install && cd ../client && npm install && npm run build",
    "server-dev": "nodemon server.js",
    "client-dev": "cd ../client && npm start",
    "client-builder": "cd ../client && nodemon --exec \"npm run build\" --ignore ./build", 
    "dev": "concurrently \"npm run client-builder\" \"npm run server-dev\" \"npm run client-dev\"",
    "dev-lite": "concurrently \"npm run server-dev\" \"npm run client-dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https:3/github.com/MotstandenWeb/MotstandenWeb.git"
  },
  "author": "Studentorchesteret den Ohmske Motstanden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MotstandenWeb/MotstandenWeb/issues"
  },
  "homepage": "https://github.com/MotstandenWeb/MotstandenWeb#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "better-sqlite3": "^7.1.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "serve-index": "^1.9.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
